{
  "name": "@dem1989/forward",
  "version": "0.1.0",
  "description": "HELSE Forward: A TypeScript framework for building agentic AI applications with tool usage and observability",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run test && npm run build",
    "examples": "ts-node examples/index.ts",
    "docs": "typedoc --out docs src --excludePrivate --excludeInternal false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dem1989/forward.git"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "writing",
    "openai",
    "tools",
    "agentic",
    "framework",
    "typescript",
    "helse",
    "forward",
    "assistant"
  ],
  "author": "dem1989 <dem1989@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dem1989/forward/issues"
  },
  "homepage": "https://github.com/dem1989/forward#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "SETUP.md",
    "examples"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "jest": "^29.7.0",
    "rimraf": "5.0.10",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typedoc": "0.25.13",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "openai": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
